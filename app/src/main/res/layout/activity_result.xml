<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ResultActivity">

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="@string/result"
        android:textSize="7pt"
        app:layout_constraintBottom_toBottomOf="@+id/resultheader"
        app:layout_constraintEnd_toEndOf="@+id/resultheader"
        app:layout_constraintStart_toStartOf="@+id/resultheader"
        app:layout_constraintTop_toTopOf="@+id/resultheader" />

    <TextView
        android:id="@+id/titleHue"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/indexhsv"
        android:textSize="7pt"
        android:layout_marginStart="15dp"
        app:layout_constraintBottom_toTopOf="@+id/block_res1"
        app:layout_constraintStart_toEndOf="@+id/img_result1"
        app:layout_constraintTop_toTopOf="@+id/img_result1" />

    <TextView
        android:id="@+id/titleHue1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/indexhsv"
        android:textSize="7pt"
        app:layout_constraintBottom_toTopOf="@+id/block_res2"
        app:layout_constraintStart_toStartOf="@id/titleHue"
        app:layout_constraintTop_toTopOf="@+id/img_result2" />

    <TextView
        android:id="@+id/titleHue2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/indexhsv"
        android:textSize="7pt"
        app:layout_constraintBottom_toTopOf="@+id/block_res3"
        app:layout_constraintStart_toStartOf="@id/titleHue"
        app:layout_constraintTop_toTopOf="@+id/img_result3" />
    <TextView
        android:id="@+id/titleHue3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/indexhsv"
        android:textSize="7pt"
        app:layout_constraintBottom_toTopOf="@+id/block_res4"
        app:layout_constraintStart_toStartOf="@id/titleHue"
        app:layout_constraintTop_toTopOf="@+id/img_result4" />

    <TextView
        android:id="@+id/concentration1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/indexconc"
        android:textSize="7pt"
        app:layout_constraintBottom_toTopOf="@+id/block_res1"
        app:layout_constraintEnd_toEndOf="@+id/block_res1"
        app:layout_constraintStart_toEndOf="@+id/titleHue"
        app:layout_constraintTop_toTopOf="@+id/img_result1" />

    <TextView
        android:id="@+id/concentration2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/indexconc"
        android:textSize="7pt"
        app:layout_constraintStart_toStartOf="@id/concentration1"
        app:layout_constraintTop_toTopOf="@id/titleHue1"/>

    <TextView
        android:id="@+id/concentration3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/indexconc"
        android:textSize="7pt"
        app:layout_constraintStart_toStartOf="@id/concentration1"
        app:layout_constraintTop_toTopOf="@id/titleHue2"/>
    <TextView
        android:id="@+id/concentration4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/indexconc"
        android:textSize="7pt"
        app:layout_constraintStart_toStartOf="@id/concentration1"
        app:layout_constraintTop_toTopOf="@id/titleHue3"/>

    <ImageView
        android:id="@+id/resultheader"
        android:layout_width="300dp"
        android:layout_height="100dp"
        app:layout_constraintBottom_toTopOf="@id/img_result1"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:srcCompat="@drawable/emptyheader1" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="35dp"
        android:layout_height="35dp"
        android:layout_marginStart="-15dp"
        app:layout_constraintBottom_toBottomOf="@+id/resultheader"
        app:layout_constraintStart_toStartOf="@+id/resultheader"
        app:layout_constraintTop_toTopOf="@id/resultheader"
        app:srcCompat="@drawable/smallcirc1" />

    <ImageView
        android:id="@+id/block_res1"
        android:layout_width="300dp"
        android:layout_height="40dp"
        app:layout_constraintBottom_toBottomOf="@+id/img_result1"
        android:layout_marginBottom="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="10dp"
        app:srcCompat="@drawable/blockres1" />

    <ImageView
        android:id="@+id/block_res2"
        android:layout_width="300dp"
        android:layout_height="40dp"
        app:layout_constraintStart_toStartOf="@id/block_res1"
        app:layout_constraintEnd_toEndOf="@id/block_res1"
        app:layout_constraintBottom_toBottomOf="@+id/img_result2"
        android:layout_marginBottom="20dp"
        app:srcCompat="@drawable/blockres1" />

    <ImageView
        android:id="@+id/block_res3"
        android:layout_width="300dp"
        android:layout_height="40dp"
        app:layout_constraintStart_toStartOf="@id/block_res1"
        app:layout_constraintEnd_toEndOf="@id/block_res1"
        app:layout_constraintBottom_toBottomOf="@+id/img_result3"
        android:layout_marginBottom="20dp"
        app:srcCompat="@drawable/blockres1" />

    <ImageView
        android:id="@+id/block_res4"
        android:layout_width="300dp"
        android:layout_height="40dp"
        app:layout_constraintStart_toStartOf="@id/block_res1"
        app:layout_constraintEnd_toEndOf="@id/block_res1"
        app:layout_constraintBottom_toBottomOf="@+id/img_result4"
        android:layout_marginBottom="20dp"
        app:srcCompat="@drawable/blockres1" />


    <Button
        android:id="@+id/btn_next"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_marginEnd="55dp"
        android:layout_marginBottom="40dp"
        android:backgroundTint="@color/enableblue"
        android:text="@string/btn_finish"
        android:textSize="8pt"
        android:padding="3pt"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <Button
        android:id="@+id/btn_prev"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:text="@string/btn_prev"
        android:layout_marginStart="55dp"
        android:layout_marginBottom="40dp"
        android:backgroundTint="@color/enableblue"
        android:textSize="8pt"
        android:padding="3pt"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/val1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="6pt"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="@id/titleHue"
        app:layout_constraintEnd_toEndOf="@id/titleHue"
        app:layout_constraintTop_toTopOf="@id/block_res1"
        app:layout_constraintBottom_toBottomOf="@id/block_res1"
        />

    <TextView
        android:id="@+id/val2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="6pt"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="@id/titleHue"
        app:layout_constraintEnd_toEndOf="@id/titleHue"
        app:layout_constraintTop_toTopOf="@id/block_res2"
        app:layout_constraintBottom_toBottomOf="@id/block_res2"
        />

    <TextView
        android:id="@+id/val3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="6pt"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="@id/titleHue"
        app:layout_constraintEnd_toEndOf="@id/titleHue"
        app:layout_constraintTop_toTopOf="@id/block_res3"
        app:layout_constraintBottom_toBottomOf="@id/block_res3"
        />

    <TextView
        android:id="@+id/val4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="6pt"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="@id/titleHue"
        app:layout_constraintEnd_toEndOf="@id/titleHue"
        app:layout_constraintTop_toTopOf="@id/block_res4"
        app:layout_constraintBottom_toBottomOf="@id/block_res4"
        />

    <TextView
        android:id="@+id/concdisp1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="6pt"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="@id/concentration1"
        app:layout_constraintEnd_toEndOf="@id/concentration1"
        app:layout_constraintTop_toTopOf="@id/block_res1"
        app:layout_constraintBottom_toBottomOf="@id/block_res1"
        />

    <TextView
        android:id="@+id/concdisp2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="6pt"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="@id/concentration1"
        app:layout_constraintEnd_toEndOf="@id/concentration1"
        app:layout_constraintTop_toTopOf="@id/block_res2"
        app:layout_constraintBottom_toBottomOf="@id/block_res2"
        />

    <TextView
        android:id="@+id/concdisp3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="6pt"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="@id/concentration1"
        app:layout_constraintEnd_toEndOf="@id/concentration1"
        app:layout_constraintTop_toTopOf="@id/block_res3"
        app:layout_constraintBottom_toBottomOf="@id/block_res3"
        />

    <TextView
        android:id="@+id/concdisp4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="6pt"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="@id/concentration1"
        app:layout_constraintEnd_toEndOf="@id/concentration1"
        app:layout_constraintTop_toTopOf="@id/block_res4"
        app:layout_constraintBottom_toBottomOf="@id/block_res4"
        />

    <ImageView
        android:id="@+id/dispres1"
        android:layout_width="95dp"
        android:layout_height="95dp"
        app:layout_constraintTop_toTopOf="@id/img_result1"
        app:layout_constraintEnd_toEndOf="@id/img_result1"
        app:layout_constraintStart_toStartOf="@id/img_result1"
        app:layout_constraintBottom_toBottomOf="@id/img_result1"
        />

    <ImageView
        android:id="@+id/dispres2"
        android:layout_width="95dp"
        android:layout_height="95dp"
        app:layout_constraintTop_toTopOf="@id/img_result2"
        app:layout_constraintEnd_toEndOf="@id/img_result2"
        app:layout_constraintStart_toStartOf="@id/img_result2"
        app:layout_constraintBottom_toBottomOf="@id/img_result2"
        />

    <ImageView
        android:id="@+id/dispres3"
        android:layout_width="95dp"
        android:layout_height="95dp"
        app:layout_constraintTop_toTopOf="@id/img_result3"
        app:layout_constraintEnd_toEndOf="@id/img_result3"
        app:layout_constraintStart_toStartOf="@id/img_result3"
        app:layout_constraintBottom_toBottomOf="@id/img_result3"
        />

    <ImageView
        android:id="@+id/dispres4"
        android:layout_width="95dp"
        android:layout_height="95dp"
        app:layout_constraintTop_toTopOf="@id/img_result4"
        app:layout_constraintEnd_toEndOf="@id/img_result4"
        app:layout_constraintStart_toStartOf="@id/img_result4"
        app:layout_constraintBottom_toBottomOf="@id/img_result4"
        />
    <ImageView
        android:id="@+id/img_result1"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginEnd="-55dp"
        app:layout_constraintBottom_toTopOf="@id/img_result2"
        app:layout_constraintEnd_toStartOf="@+id/block_res1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/resultheader"
        app:srcCompat="@drawable/prevres1trans" />

    <ImageView
        android:id="@+id/img_result2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginEnd="-55dp"
        app:layout_constraintBottom_toTopOf="@id/img_result3"
        app:layout_constraintEnd_toStartOf="@+id/block_res1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_result1"
        app:srcCompat="@drawable/prevres1trans" />

    <ImageView
        android:id="@+id/img_result3"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginEnd="-55dp"
        app:layout_constraintBottom_toTopOf="@id/img_result4"
        app:layout_constraintEnd_toStartOf="@+id/block_res1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_result2"
        app:srcCompat="@drawable/prevres1trans" />

    <ImageView
        android:id="@+id/img_result4"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginEnd="-55dp"
        app:layout_constraintBottom_toTopOf="@id/btn_prev"
        app:layout_constraintEnd_toStartOf="@+id/block_res1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_result3"
        android:layout_marginBottom="10dp"
        app:srcCompat="@drawable/prevres1trans" />

</androidx.constraintlayout.widget.ConstraintLayout>